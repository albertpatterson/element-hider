{
  "manifest_version": 2,
  "name": "medium-helper",
  "version": "1.0.0",
  "description": "a helper to improve medium.com",
  "browser_action": {
    "default_title": "Medium Helper",
    "default_icon": "icon.png"
  },
  "commands": {},
  "content_scripts": [
    {
      "matches": [
        "https://medium.com/*",
        "http://medium.com/*"
      ],
      "js": [
        "injected/injected-bundle.js"
      ]
    }
  ],
  "homepage_url": "https://github.com/albertpatterson/medium-helper",
  "permissions": []
}